<Window x:Class="InitialProject.View.AddFilters"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InitialProject.View"
        mc:Ignorable="d"
        Title="AddFilters" Height="450" Width="800">
    <Grid>
        <ComboBox Width="100" Height="20" x:Name="Filters">
            <ComboBoxItem Content="Name" x:Name="TourLocation">
            </ComboBoxItem>
            <ComboBoxItem Content="Location" x:Name="TourDuration">
            </ComboBoxItem>
            <ComboBoxItem Content="Type" x:Name="TourLanguage">
            </ComboBoxItem>
            <ComboBoxItem Content="GuestNumber" x:Name="TourGuestNumber">
            </ComboBoxItem>
        </ComboBox>

        <Button Content="Done" Click="Button_Click_Done" Width="60" Height="20" Margin="70, 150, 0, 0">
        </Button>
    </Grid>
</Window>
